window.Adapta_RGPD = window.Adapta_RGPD || {};
Adapta_RGPD.getCookie= function(name) {
var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
return v ? v[2] : null;
}
Adapta_RGPD.setCookie= function (name, value) {
var date = new Date();
